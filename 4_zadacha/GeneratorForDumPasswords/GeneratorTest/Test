using GeneratorForDumPasswords.View;
using GeneratorForDumPasswords.Model;
using GeneratorForDumPasswords.Controler;
using GeneratorForDumPasswords.Service;
using GeneratorForDumPasswords;

namespace GeneratorTest
{
    public class Tests
    {
        [Test]
        public void Test()
        {
            string[] answear =
            {
                "11aa2", "11ab2", "11ba2", "11bb2"
            };
            int i = 0;
            GeneratorModel model = new GeneratorModel(2, 2);
            GeneratorService services = new GeneratorService();
            GeneratorControler controller = new GeneratorControler(services);
            GeneratorView view = new GeneratorView(controller);
            List<string> testList = services.GetAllPasswords(model);
            foreach (string test in testList)
            {
                Assert.AreEqual(answear[i], test);
                i++;
            }
        }
    }
}
